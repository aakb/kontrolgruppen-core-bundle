<div class="border">
    <h2>{{ 'process_report.report.journal.title'|trans }}</h2>
    <table>
        {% for journalEntry in journalEntries %}
        <tr>
            <td>{{ journalEntry.createdAt|localizeddate('short', 'none') }}</td>
            <td>{{ journalEntry.title }}</td>
        </tr>
        <tr>
            <td colspan="2">{{ journalEntry.body }}</td>
        </tr>
        {% endfor %}
    </table>
</div>
