<table>
    <thead>
    <tr>
        <th>{{ 'process_report.report.economy.income.type'|trans }}</th>
        <th>{{ 'process_report.report.economy.income.period'|trans }}</th>
        <th>{{ 'process_report.report.economy.income.amount'|trans }}</th>
        <th>{{ 'process_report.report.economy.income.amount_period'|trans }}</th>
    </tr>
    </thead>
    <tbody>
    {% for economyEntry in economyEntries %}
        <tr>
            <td>{{ economyEntry.incomeType.name }}</td>
            <td>{{ economyEntry.periodFrom|localizeddate('short', 'none') }} - {{ economyEntry.periodTo|localizeddate('short', 'none') }}</td>
            <td>{{ economyEntry.amount|localizedcurrency('DKK') }}</td>
            <td>{{ enumTranslation(economyEntry.amountPeriod, 'EconomyEntryAmountPeriodEnumType') }}</td>
        </tr>
    {% else %}
        <tr>
            <td colspan="3">{{ 'common.no_records_found'|trans }}</td>
        </tr>
    {% endfor %}
    </tbody>
</table>
