<div class="modal fade" id="journalQuickviewModal" tabindex="-1" role="dialog" aria-labelledby="journalQuickviewModal" aria-hidden="true">
  <div class="modal-dialog" role="document">
    <div class="modal-content">
      <div class="modal-header">
        <h5 class="modal-title" id="exampleModalLabel">{{ 'journal_quick_view.title'|trans }}</h5>
        <button type="button" class="close" data-dismiss="modal" aria-label="{{ 'common.actions.close'|trans }}">
          <span aria-hidden="true">&times;</span>
        </button>
      </div>
      <div class="d-flex justify-content-center">
        <div class="spinner-border spinner-border-sm js-journal-entry-modal-spinner" role="status" style="display: none">
          <span class="sr-only">Loading...</span>
        </div>
      </div>
      <div class="modal-body js-journal-entry-modal-content">
      </div>
      <div class="modal-footer">
        <a href="{{ path('journal_entry_index', {'process': 0}) }}" class="btn btn-dark js-journal-entry-modal-show-all-link" data-toggle="tooltip" data-placement="top" title="{{ 'journal_quick_view.actions.show'|trans }}">
          <i class="fas {{ iconClass('show') }} fa-inverse"></i>
        </a>
      </div>
    </div>
  </div>
</div>
