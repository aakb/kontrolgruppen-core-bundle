<div class="mb-3">
  <div class="row">
    <div class="col">
        <p>{{ 'process.summary.title'|trans }}</p>
        <span class="d-block text-muted"><span class="sr-only">{{ 'process.table.client_cpr'|trans }}</span>{{ process.clientCPR }}</span>
        <span class="d-block text-muted"><span class="sr-only">{{ 'process.table.client_name'|trans }}</span>{{ process.client.firstName }} {{ process.client.lastName }}</span>
        <span class="d-block text-muted"><span class="sr-only">{{ 'process.table.client_adress'|trans }}</span>{{ process.client.address }}, {{ process.client.postalCode }} {{ process.client.city }}</span>
        <span class="d-block text-muted"><span class="sr-only">{{ 'process.table.client_telephone'|trans }}</span>{{ process.client.telephone }}</span>
    </div>
  </div>
</div>
